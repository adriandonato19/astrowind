<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css"></link>


<body class="bg-gray-100">
    <section class="py-12">
        <div class="container mx-auto px-4">
            <h2 class="text-4xl font-bold text-center text-gray-800 mb-8">Nuestros abogados</h2>
            <div class="flex flex-wrap justify-center">
                <!-- Miembro del equipo 1 -->
                <div class="w-full sm:w-1/2 lg:w-1/3 p-4">
                    <div class="bg-white rounded-lg shadow-lg overflow-hidden transform transition-transform duration-300 hover:scale-105 hover:shadow-2xl">
                        <div class="w-full aspect-w-3 aspect-h-4">
                            <img class="w-full h-full object-contain" src="src/assets/images/Enrique.jpg" alt="Enrique Fernández">
                        </div>
                        <div class="p-6">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css"></link>

                            <h3 class="text-2xl font-semibold text-gray-800">Enrique Fernández Osorio</h3>
                            <p class="text-gray-600">Abogado</p>
                            <p class="mt-3 text-gray-600">Áreas de práctica y experiencia:</p>
                            <ul class="mt-3 text-gray-600 list-disc list-inside">
                                <li>Mercantil, Sociedades y Derecho Comercial en general</li>
                                <li>Derecho Laboral</li>
                                <li>Derecho Civil</li>
                                <li>Derecho Administrativo</li>
                            </ul>
                            <p class="mt-3 text-gray-600">Idiomas: Español</p>
                            <div class="mt-4">
                                <a href="https://www.linkedin.com" class="text-gray-600 hover:text-blue-500">
                                    <i class="fab fa-linkedin fa-2x"></i>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Miembro del equipo 2 -->
                <div class="w-full sm:w-1/2 lg:w-1/3 p-4">
                    <div class="bg-white rounded-lg shadow-lg overflow-hidden transform transition-transform duration-300 hover:scale-105 hover:shadow-2xl">
                        <div class="w-full aspect-w-3 aspect-h-4">
                            <img class="w-full h-full object-contain" src="src/assets/images/Diana.JPG" alt="Diana Centella">
                        </div>
                        <div class="p-6">
                            <h3 class="text-2xl font-semibold text-gray-800">Diana Centella Castillo</h3>
                            <p class="text-gray-600">Abogada</p>
                            <p class="mt-3 text-gray-600">Áreas de práctica y experiencia:</p>
                            <ul class="mt-3 text-gray-600 list-disc list-inside">
                                <li>Derecho Penal</li>
                                <li>Derecho Civil</li>
                                <li>Derecho de Familia</li>
                            </ul>
                            <p class="mt-3 text-gray-600">Idiomas: Español e inglés</p>
                            <div class="mt-4">
                                <a href="https://www.linkedin.com" class="text-gray-600 hover:text-blue-500">
                                    <i class="fab fa-linkedin fa-2x"></i>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
